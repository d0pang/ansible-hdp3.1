#VERSION_NUMBER=2.6.4.0-91
[HDP-{{ hdp_version.split('-')[0] }}-GPL]
name=HDP-GPL Version - HDP-GPL-{{ hdp_version }}
baseurl=http://{{ groups['repo'][0] }}/hdp/HDP-GPL/centos{{ ansible_distribution_major_version }}/{{ hdp_version }}
gpgcheck=1
gpgkey=http://{{ groups['repo'][0] }}/hdp/HDP-GPL/centos{{ ansible_distribution_major_version }}/{{ hdp_version }}/RPM-GPG-KEY/RPM-GPG-KEY-Jenkins
enabled=1
priority=1